        </div>

        <div class="modal fade modal-flex show" id="modelorder" runat="server" tabindex="-1" style="display: block; padding-left: 17px; overflow-x: hidden; overflow-y: auto; display:none;">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-body model-container">
                        <div class="modal-header">
                            <h5 class="modal-title">Case History Order</h5>
                            <a id="lnkorderexit" class="close" data-dismiss="modal" aria-label="Close" ng-click="hideOrderDetailDiv();"><span aria-hidden="true">&times;</span></a>
